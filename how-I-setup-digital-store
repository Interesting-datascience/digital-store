   13  sudo groupadd docker
   14  sudo usermod -aG docker $USER
   25  sudo apt install python3
   32  mkdir cloud-native
   34  cd cloud-native/
   79  pip install --user pipenv cookiecutter autopep8
   39  sudo update-alternatives --install /usr/bin/python python /usr/bin/python3.6m
   49  mkdir digital-store
