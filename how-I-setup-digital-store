   52  cookiecutter https://github.com/awesto/cookiecutter-django-shop
   54  sudo update-alternatives --install /usr/bin/python python /usr/bin/python3.6 2
   57  sudo apt install git
   88  docker-compose up --build -d
  104  sudo apt install curl
